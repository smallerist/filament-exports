{
  "name": "smallerid/filament-exports",
  "description": "Customizable exports for Filament Admin Panel",
  "autoload": {
    "psr-4": {
      "SmallerId\\FilamentExport\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SmallerId\\FilamentExport\\Tests\\": "tests/src/",
      "SmallerId\\FilamentExport\\Tests\\Database\\Factories\\": "tests/database/factories/"
    }
  },
  "authors": [
    {
      "name": "purwantoid",
      "email": "purwanto@smaller.id"
    }
  ],
  "require": {
    "php": "^8.0",
    "filament/tables": "^3.2",
    "barryvdh/laravel-dompdf": "^2.0",
    "spatie/simple-excel": ">=3.2.0",
    "phpoffice/phpword": "^1.3"
  },
  "require-dev": {
    "orchestra/testbench": "^8.5",
    "pestphp/pest": "^2.1",
    "pestphp/pest-plugin-livewire": "^2.1",
    "filament/filament": "^3.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "SmallerId\\FilamentExport\\FilamentExportServiceProvider"
      ]
    }
  },
  "license": "MIT",
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "beta"
}
